<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions 
  PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>
  <definition name=".pageWithMenuLayout" template="/WEB-INF/jsp/layout/pageWithMenu.jsp">
    <put-attribute name="header" value="/WEB-INF/jsp/layout/header.jsp" />
    <put-attribute name="footer" value="/WEB-INF/jsp/layout/footer.jsp" />
    <put-attribute name="menu" value="/menu/menu.do" />
    <put-attribute name="body" value="/hello/example10.jsp" />
  </definition>
  
  <definition name=".pageWithMenu2.*.*" extends=".pageWithMenuLayout">
    <put-attribute name="body" value="/WEB-INF/jsp/{1}/{2}.jsp" />
    <put-attribute name="module" value="{1}" />
    <put-attribute name="pagePrefix" value="{1}.{2}." />
  </definition>
  
  <definition name=".pageWithMenu3.*.*.*" extends=".pageWithMenuLayout">
    <put-attribute name="body" value="/WEB-INF/jsp/{1}/{2}/{3}.jsp" />
    <put-attribute name="module" value="{1}.{2}" />
    <put-attribute name="pagePrefix" value="{1}.{2}.{3}." />
  </definition>
  
  <definition name=".standardPageWithMenu3.*.*.*standard" extends=".pageWithMenuLayout">
    <put-attribute name="body" value="/WEB-INF/jsp/crud/{3}.jsp" />
    <put-attribute name="module" value="{1}.{2}" />
    <put-attribute name="pagePrefix" value="{1}.{2}.{3}." />
  </definition>
  
  <definition name=".pageWithMenu4.*.*.*.*" extends=".pageWithMenuLayout">
    <put-attribute name="body" value="/WEB-INF/jsp/{1}/{2}/{3}/{4}.jsp" />
    <put-attribute name="module" value="{1}.{2}.{3}" />
    <put-attribute name="pagePrefix" value="{1}.{2}.{3}.{4}." />
  </definition>
  
  <definition name=".standardPageWithMenu4.*.*.*.*standard" extends=".pageWithMenuLayout">
    <put-attribute name="body" value="/WEB-INF/jsp/crud/{4}.jsp" />
    <put-attribute name="module" value="{1}.{2}.{3}" />
    <put-attribute name="pagePrefix" value="{1}.{2}.{3}.{4}." />
  </definition>
  
  <definition name=".loginLayout" template="/WEB-INF/jsp/layout/loginLayout.jsp">
    <put-attribute name="header" value="/WEB-INF/jsp/layout/header.jsp" />
    <put-attribute name="footer" value="/WEB-INF/jsp/layout/footer.jsp" />
  </definition>
  
  <definition name=".loginLayout.*.*" extends=".loginLayout">
    <put-attribute name="body" value="/WEB-INF/jsp/{1}/{2}.jsp" />
    <put-attribute name="pagePrefix" value="{1}.{2}." />
  </definition>
  
  <definition name=".pageTitle" template="/WEB-INF/jsp/layout/title.jsp" />
  
  <definition name=".customEdit.footer.fillPatternCoordinates" template="/WEB-INF/jsp/inventory/fillpattern/editFillPatternCoordinates.jsp" />
  <definition name=".customDetails.footer.fillPatternCoordinates" template="/WEB-INF/jsp/inventory/fillpattern/viewFillPatternCoordinates.jsp" />
  <definition name=".customEdit.footer.queries" template="/WEB-INF/jsp/report/query/editQueries.jsp" />
  <definition name=".customDetails.footer.queries" template="/WEB-INF/jsp/report/query/viewQueries.jsp" />
  <definition name=".customEdit.containerBaseNames" template="/WEB-INF/jsp/inventory/container/containerBaseNames.jsp" />
  <definition name=".customEdit.quantities" template="/WEB-INF/jsp/inventory/quantity/editQuantities.jsp" />
  <definition name=".customDetails.quantities" template="/WEB-INF/jsp/inventory/quantity/viewQuantities.jsp" />
  <definition name=".customEdit.notes" template="/WEB-INF/jsp/crud/notes.jsp" />
  <definition name=".customDetails.notes" template="/WEB-INF/jsp/crud/notes.jsp" />
  <definition name=".customDetails.filedownload" template="/WEB-INF/jsp/common/document/filedownload.jsp" />
  
</tiles-definitions>