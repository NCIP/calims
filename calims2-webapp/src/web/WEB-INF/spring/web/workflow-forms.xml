<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
  xmlns:util="http://www.springframework.org/schema/util"
  xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/util 
        http://www.springframework.org/schema/util/spring-util-3.0.xsd">
   
  <bean class="gov.nih.nci.calims2.ui.generic.crud.CRUDFormDecorator" id="projectFormDecorator">
    <property name="privilegeEvaluator" ref="privilegeEvaluator"/>
    <property name="form">
      <bean class="gov.nih.nci.calims2.uic.descriptor.form.Form" p:id="projectForm">
        <property name="children">
          <list>
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.FormTab" p:name="main">
              <property name="children">
                <list>
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.TextField" p:id="name" p:name="entity.name" p:required="true" p:value="form.entity.name" />
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.TextField" p:id="displayName" p:name="entity.displayName" p:required="true" p:value="form.entity.displayName" />
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.TextAreaField" p:id="description" p:name="entity.description" p:value="form.entity.description" />
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SingleSelectField" p:collectionType="ENUMERATIONS" p:id="status" p:name="status"  p:options="projectStatuses" p:required="true" p:selected="form.entity.status"/>
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SingleSelectField" p:id="types" p:name="typeId"  p:required="true" p:properties="id:id,label:name" p:options="types" p:selected="form.entity.type" /> 
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.DateField" p:id="actualStartDate" p:required="true" p:name="entity.actualStartDate" p:value="form.entity.actualStartDate"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.DateField" p:id="actualEndDate" p:required="true" p:name="entity.actualEndDate" p:value="form.entity.actualEndDate"/>
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.DateField" p:id="expectedStartDate" p:required="true" p:name="entity.expectedStartDate" p:value="form.entity.expectedStartDate"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.DateField" p:id="expectedEndDate" p:required="true" p:name="entity.expectedEndDate" p:value="form.entity.expectedEndDate"/>
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.CheckBoxField" p:id="isTemplate" p:name="entity.isTemplate" p:value="form.entity.isTemplate"/>
                  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.FieldSeparator" />
                  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="approvals" p:name="approvalIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="approvals" p:selected="form.entity.approvalCollection" p:optionCreatorCallback="nameCallback" p:selectKey="approvalSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="collaborations" p:name="collaborationIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="collaborations" p:selected="form.entity.collaborationCollection" p:optionCreatorCallback="nameCallback" p:selectKey="collaborationSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="equipments" p:name="equipmentIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="equipments" p:selected="form.entity.equipmentCollection" p:optionCreatorCallback="nameCallback" p:selectKey="equipmentSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="laboratories" p:name="laboratoryIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="laboratories" p:selected="form.entity.laboratoryCollection" p:optionCreatorCallback="nameCallback" p:selectKey="laboratorySelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="labSupplies" p:name="labSupplyIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="labSupplies" p:selected="form.entity.labSupplyCollection" p:optionCreatorCallback="nameCallback" p:selectKey="labSupplySelectKey"/>            
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="methods" p:name="methodIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="methods" p:selected="form.entity.methodCollection" p:optionCreatorCallback="nameCallback" p:selectKey="methodSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="organizations" p:name="organizationIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="organizations" p:selected="form.entity.organizationCollection" p:optionCreatorCallback="nameCallback" p:selectKey="organizationSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="persons" p:name="personIds" p:idProperty="id" p:required="true" p:labelProperty="familyName" p:multiple="true" p:options="persons" p:selected="form.entity.personCollection" p:optionCreatorCallback="personCallback" p:selectKey="personSelectKey"/>         
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="reports" p:name="reportIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="reports" p:selected="form.entity.reportCollection" p:optionCreatorCallback="nameCallback" p:selectKey="reportSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="softwares" p:name="sofwareIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="softwares" p:selected="form.entity.softwareCollection" p:optionCreatorCallback="nameCallback" p:selectKey="softwareSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="specimens" p:name="specimenIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="specimens" p:selected="form.entity.specimenCollection" p:optionCreatorCallback="nameCallback" p:selectKey="specimenSelectKey"/>  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.SelectField" p:id="worksheets" p:name="worksheetIds" p:idProperty="id" p:labelProperty="name" p:multiple="true" p:options="worksheets" p:selected="form.entity.worksheetCollection" p:optionCreatorCallback="nameCallback" p:selectKey="worksheetSelectKey"/>  

                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.FieldSeparator" />
                  
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.TextField" p:id="createdBy" p:name="entity.createdBy" p:selector="details" p:value="form.entity.createdBy" />
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.DateTimeField" p:id="dateCreated" p:name="entity.dateCreated" p:selector="details" p:value="form.entity.dateCreated" />
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.TextField" p:id="modifiedBy" p:name="entity.modifiedBy" p:selector="details" p:value="form.entity.modifiedBy" />
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.DateTimeField" p:id="dateModified" p:name="entity.dateModified" p:selector="details" p:value="form.entity.dateModified" />
                </list>
              </property>
            </bean> 
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.FormTab" p:name="notes">
              <property name="children">
                <list>
                  <bean class="gov.nih.nci.calims2.uic.descriptor.form.CustomField" p:customSelector="notes" />
                </list>
              </property>
            </bean>      
          </list>
        </property>
        <property name="commands">
          <list>
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="save" p:selector="edit" p:type="submit" p:url="/workflow/project/save.do" p:validate="true" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="cancel" p:selector="edit" p:type="submit" p:url="/workflow/project/cancelSave.do" /> 
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createApproval" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=0" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createCollaboration" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=1" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createEquipment" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=2" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createLaboratory" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=3" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createLabSupply" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=4" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createMethod" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=5" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createOrganization" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=6" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createPerson" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=7" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createReport" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=8" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createSoftware" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=9" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createSpecimen" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=10" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createWorksheet" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=11" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="createType" p:selector="edit" p:tab="main" p:type="submit" p:url="/workflow/project/call.do?subFlowId=12" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="manage" p:selector="details" p:type="submit" p:url="/workflow/project/manage.do" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="list" p:selector="details" p:type="submit" p:url="/workflow/project/list.do" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.command.Command" p:name="edit" p:selector="details" p:type="submit" p:url="/workflow/project/edit.do" />
          </list>
        </property>
        <property name="hiddenFields">
          <list>
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.HiddenField" p:name="flowContext" p:value="flowContext" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.HiddenField" p:name="currentView" p:value="form.currentView" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.HiddenField" p:name="returnView" p:selector="edit" p:value="form.returnView" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.HiddenField" p:name="entity.id" p:selector="edit" p:value="form.entity.id" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.HiddenField" p:name="editMode" p:selector="edit" p:value="form.editMode" />
            <bean class="gov.nih.nci.calims2.uic.descriptor.form.HiddenField" p:name="id" p:value="form.entity.id" />
          </list>
        </property>
      </bean>
    </property> 
  </bean>
  
 
  


</beans>