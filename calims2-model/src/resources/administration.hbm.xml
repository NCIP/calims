<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findPersonWithEmail">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where person.id is not null and emailAddress is not null]]>
  </query>
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findByCollaborationId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where collaboration.id = :id]]>
  </query>
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findByIds">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where id in(:ids)]]>
  </query>
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findByLaboratoryId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where laboratory.id = :id]]>
  </query>
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findByLocationId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where location.id = :id]]>
  </query> 
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findByOrganizationId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where organization.id = :id]]>
  </query>
  <query name="gov.nih.nci.calims2.domain.administration.ContactInformation.findByPersonId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.ContactInformation where person.id = :id]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.customerservice.Service.findByCustomerRequestId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.customerservice.Service where customerRequest.id = :customerRequestId]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.customerservice.Service.findTemplates">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.customerservice.Service where isTemplate = true order by name]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.Equipment.findByName">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.Equipment eq where eq.class = gov.nih.nci.calims2.domain.administration.Equipment order by name]]>
  </query>
 
  <query name="gov.nih.nci.calims2.domain.administration.NamingConvention.findByLaboratoryIdAndType">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.NamingConvention 
             where (laboratory.id is null or laboratory.id = :laboratoryId) and type = :type]]>
  </query>
  <query name="gov.nih.nci.calims2.domain.administration.NamingConvention.findByType">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.NamingConvention where type = :type]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.Person.findByUserName">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.Person where userName = :userName]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.PostalAddress.findByContactInformationId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.PostalAddress where contactInformation.id = :id]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.Role.findByRoleType">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.Role where type = :roleType]]>
  </query>
  

  <query name="gov.nih.nci.calims2.domain.administration.StandardUnit.findByTypeId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.StandardUnit where type.id = :typeId]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.TelephoneNumber.findByContactInformationId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.TelephoneNumber where contactInformation.id = :id]]>
  </query>
 
  <query name="gov.nih.nci.calims2.domain.administration.Word.findByNamingConventionId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.Word where namingConvention.id = :namingConventionId]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.customerservice.AccountNumber.findByAccountId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.customerservice.AccountNumber where account.id = :accountId]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.customerservice.ServiceItem.findByServiceId">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.customerservice.ServiceItem where service.id = :serviceId]]>
  </query>
  
  <query name="gov.nih.nci.calims2.domain.administration.customerservice.CustomerRequest.findByCreator">
    <![CDATA[from gov.nih.nci.calims2.domain.administration.customerservice.CustomerRequest
    where createdBy = :creator order by name]]>
  </query>
  
  
</hibernate-mapping>