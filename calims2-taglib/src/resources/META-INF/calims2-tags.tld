  <tag>
    <name>button</name>
    <tag-class>gov.nih.nci.calims2.taglib.form.ButtonTag</tag-class>
    <body-content>JSP</body-content>
    <description>
      A tag which include a dojo.form.Button. The body of the tag is rendered in the Button.
    </description>
    
    <attribute>
      <name>alt</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The alt attribute of the input.
      </description>
    </attribute>
    
    <attribute>
      <name>altKey</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The key for the alt attribute of the input. The message specified by the key has precedence over the message specified directly.
      </description>
    </attribute>
    
    <attribute>
      <name>checked</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        For radio button and checkboxes only. Tells if the checkbox or radio button is checked.
      </description>
    </attribute>
    
    <attribute>
      <name>disabled</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The disabled attribute of the input. 
      </description>
    </attribute>
    
    <attribute>
      <name>iconClass</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The value of the iconClass attribute.
      </description>
    </attribute>
    
    <attribute>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The id attribute of the input. 
      </description>
    </attribute>
    
    <attribute>
      <name>labelKey</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The value of the key of the button label message.
      </description>
    </attribute>
    
    <attribute>
      <name>label</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The value of the button label message. The message specified by the key has precedence over the message specified directly.
      </description>
    </attribute>
    
    <attribute>
      <name>name</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The name of the input.
      </description>
    </attribute>
    
    <attribute>
      <name>readOnly</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The readOnly attribute of the input. 
      </description>
    </attribute>
    
    <attribute>
      <name>style</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The style attribute of the input.
      </description>
    </attribute>
    
     <attribute>
      <name>styleClass</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The class attribute of the input.
      </description>
    </attribute>
    
    <attribute>
      <name>tabIndex</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The tabIndex attribute of the input. 
      </description>
    </attribute>
    
    <attribute>
      <name>type</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The type of button. The default value is submit.
        Possible values are button, checkbox, radio, reset, and submit
      </description>
    </attribute>
    
    <attribute>
      <name>url</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The url to set in the form when the button is clicked.
      </description>
    </attribute>
    
    <attribute>
      <name>validate</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The value to give to the validateFlag of the form when a url is specified. Default value is false
      </description>
    </attribute>
    
    <attribute>
      <name>value</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        The value of the input. 
      </description>
    </attribute>
    
  </tag>
  
